{
  "files.eol": "\n",
  "deno.enable": true,
  "deno.lint": true,
  "deno.enablePaths": [
    "."
  ],
  "deno.disablePaths": [
    "./tests/fixtures/http"
  ],
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  // Explicit overrides for VS Codeâ€™s own config files
  "[jsonc][file:.vscode/settings.json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc][file:.vscode/launch.json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc][file:.vscode/extensions.json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc][file:.vscode/tasks.json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "eslint.enable": false,
  "prettier.enable": false
}